@article{soruco2015,
  title = {Contribution of Glacier Runoff to Water Resources of {{La Paz}} City, {{Bolivia}} (16\textdegree{} {{S}})},
  author = {Soruco, Alvaro and Vincent, Christian and Rabatel, Antoine and Francou, Bernard and Thibert, Emmanuel and Sicart, Jean Emmanuel and Condom, Thomas},
