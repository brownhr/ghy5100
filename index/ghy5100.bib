@article{molina2015,
  title = {Multidecadal Change in Streamflow Associated with Anthropogenic Disturbances in the Tropical {{Andes}}},
  author = {Molina, A. and Vanacker, V. and Brisson, E. and Mora, D. and Balthazar, V.},
  date = {2015-10-20},
  journaltitle = {Hydrology and Earth System Sciences},
  shortjournal = {Hydrol. Earth Syst. Sci.},
  volume = {19},
  number = {10},
  pages = {4201--4213},
  issn = {1607-7938},
  doi = {10.5194/hess-19-4201-2015},
  url = {https://hess.copernicus.org/articles/19/4201/2015/},
  urldate = {2022-02-21},
  abstract = {Andean headwater catchments are an important source of freshwater for downstream water users. However, few long-term studies exist on the relative importance of climate change and direct anthropogenic perturbations on flow regimes in these catchments. In this paper, we assess change in streamflow based on long time series of hydrometeorological data (1974\textendash 2008) and land cover reconstructions (1963\textendash 2009) in the Pangor catchment (282 km2) located in the tropical Andes. Three main land cover change trajectories can be distinguished during the period 1963\textendash 2009: (1) expansion of agricultural land by an area equal to 14\% of the catchment area (or 39 km2) in 46 years' time, (2) deforestation of native forests by 11 \% (or -31 km2) corresponding to a mean rate of 67 ha yr-1, and (3) afforestation with exotic species in recent years by about 5 \% (or 15 km2). Over the time period 1963\textendash 2009, about 50 \% of the 64 km2 of native forests was cleared and converted to agricultural land. Given the strong temporal variability of precipitation and streamflow data related to El Ni\~no\textendash Southern Oscillation, we use empirical mode decomposition techniques to detrend the time series. The long-term increasing trend in rainfall is remarkably different from the observed changes in streamflow, which exhibit a decreasing trend. Hence, observed changes in streamflow are not the result of long-term change in precipitation but very likely result from anthropogenic disturbances associated with land cover change.},
  langid = {english},
  file = {C\:\\Users\\brownhr\\Zotero\\storage\\DV5SH3UJ\\molina2015.pdf}
}

